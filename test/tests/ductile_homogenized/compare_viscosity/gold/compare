set key autotitle columnhead
set datafile sep ','
set key right top
set xlabel 'strain'
set ylabel 'stress'
plot 'stress_strain_psic_2.6_c_100_eta_0.csv' using 4:3 with lines lw 3 lc rgb 'red' title 'psic = 2.6 c = 100, eta = 0', \
     'stress_strain_psic_2.6_c_100_eta_100.csv' using 4:3 with lines lw 3 lc rgb 'red' title 'psic = 2.6 c = 100, eta = 100', \
     'stress_strain_psic_2.6_c_100_eta_200.csv' using 4:3 with lines lw 3 lc rgb 'red' title 'psic = 2.6 c = 100, eta = 200', \
     'stress_strain_psic_2.6_c_100_eta_300.csv' using 4:3 with lines lw 3 lc rgb 'red' title 'psic = 2.6 c = 100, eta = 300', \
     'stress_strain_psic_2.4_c_120_eta_0.csv' using 4:3 with lines lw 3 lc rgb 'blue' title 'psic = 2.4 c = 120, eta = 0', \
     'stress_strain_psic_2.4_c_120_eta_100.csv' using 4:3 with lines lw 3 lc rgb 'blue' title 'psic = 2.4 c = 120, eta = 100', \
     'stress_strain_psic_2.4_c_120_eta_200.csv' using 4:3 with lines lw 3 lc rgb 'blue' title 'psic = 2.4 c = 120, eta = 200', \
     'stress_strain_psic_2.4_c_120_eta_300.csv' using 4:3 with lines lw 3 lc rgb 'blue' title 'psic = 2.4 c = 120, eta = 300', \
     'stress_strain_psic_2.2_c_140_eta_0.csv' using 4:3 with lines lw 3 lc rgb 'purple' title 'psic = 2.2 c = 140, eta = 0', \
     'stress_strain_psic_2.2_c_140_eta_100.csv' using 4:3 with lines lw 3 lc rgb 'purple' title 'psic = 2.2 c = 140, eta = 100', \
     'stress_strain_psic_2.2_c_140_eta_200.csv' using 4:3 with lines lw 3 lc rgb 'purple' title 'psic = 2.2 c = 140, eta = 200', \
     'stress_strain_psic_2.2_c_140_eta_300.csv' using 4:3 with lines lw 3 lc rgb 'purple' title 'psic = 2.2 c = 140, eta = 300'
pause -1 "Hit any key to quit"
