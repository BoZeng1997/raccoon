set key autotitle columnhead
set datafile sep ','
set key right top
set xlabel 'strain'
set ylabel 'stress'
plot 'stress_strain_c_0.csv' using 4:3 with lines lw 3 title 'c = 0', \
     'stress_strain_c_20.csv' using 4:3 with lines lw 3 title 'c = 20', \
     'stress_strain_c_40.csv' using 4:3 with lines lw 3 title 'c = 40', \
     'stress_strain_c_60.csv' using 4:3 with lines lw 3 title 'c = 60', \
     'stress_strain_c_80.csv' using 4:3 with lines lw 3 title 'c = 80', \
     'stress_strain_c_100.csv' using 4:3 with lines lw 3 title 'c = 100', \
     'stress_strain_c_120.csv' using 4:3 with lines lw 3 title 'c = 120', \
     'stress_strain_c_140.csv' using 4:3 with lines lw 3 title 'c = 140'
pause -1 "Hit any key to quit"
