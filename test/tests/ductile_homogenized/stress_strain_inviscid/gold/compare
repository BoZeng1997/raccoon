set key autotitle columnhead
set datafile sep ','
set key right top
set xlabel 'strain'
set ylabel 'stress'
plot 'stress_strain_psic_4.3_c_0.csv' using 4:3 with lines lw 3 title 'psic = 4.3 c = 0', \
